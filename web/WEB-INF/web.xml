<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ListAction</servlet-name>
        <servlet-class>com.action.ListAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SaveAction</servlet-name>
        <servlet-class>com.action.SaveAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteAction</servlet-name>
        <servlet-class>com.action.DeleteAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditAction</servlet-name>
        <servlet-class>com.action.EditAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateAction</servlet-name>
        <servlet-class>com.action.UpdateAction</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DisplayPhotoServlet</servlet-name>
        <servlet-class>com.action.DisplayPhotoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowAction</servlet-name>
        <servlet-class>com.action.ShowAction</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListAction</servlet-name>
        <url-pattern>/ListAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaveAction</servlet-name>
        <url-pattern>/SaveAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteAction</servlet-name>
        <url-pattern>/DeleteAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditAction</servlet-name>
        <url-pattern>/EditAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateAction</servlet-name>
        <url-pattern>/UpdateAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DisplayPhotoServlet</servlet-name>
        <url-pattern>/DisplayPhotoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowAction</servlet-name>
        <url-pattern>/ShowAction</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
